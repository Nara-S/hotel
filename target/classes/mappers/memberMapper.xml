<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.itwillbs.mapper.MemberMapper">
	<insert id="insertMember">
		insert into member(member_id,member_pass,member_name,member_birth,member_address,member_phone,member_mail,member_gender,member_grade) values(#{member_id},#{member_pass},#{member_name},#{member_birth},#{member_address},#{member_phone},#{member_mail},#{member_gender},#{member_grade})
	</insert>
	
</mapper>